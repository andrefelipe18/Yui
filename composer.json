{
    "name": "yuiframework/core",
    "description": "Core of Yui Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "andrefelipe18",
            "email": "dominguesstroppa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Yui\\Tests\\": "tests",
            "Yui\\Core\\": "core"
        },
        "files": [
            "core/Helpers/Functions/ConsolePrinter.php",
            "core/Helpers/Functions/Logger.php"
        ]
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-drift": "^2.6",
        "friendsofphp/php-cs-fixer": "^3.54",
        "phpstan/phpstan": "^1.10",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "fakerphp/faker": "^1.23"
    }
}
